{
  "name": "crud-react-demo-app",
  "version": "0.1.0",
  "dependencies": {
    "bootstrap": "^4.1.1",
    "lodash": "^4.17.10",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "react": "^16.4.1",
    "react-datepicker": "^1.5.0",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-redux-toastr": "^7.3.0",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts-ts": "2.16.0",
    "react-select": "^2.0.0-beta.7",
    "reactstrap": "^6.3.0",
    "redux": "^4.0.0",
    "redux-form": "^7.4.2",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-saga": "^0.16.0",
    "svg-sprite-generator": "^0.0.7"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "yarn build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-ts": "react-scripts-ts start",
    "start": "npm-run-all -p watch-css start-ts",
    "build": "yarn build-css && react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom --watchAll",
    "test:coverage": "react-scripts-ts test --env=jsdom --coverage",
    "eject": "react-scripts-ts eject",
    "generate:icons": "rm src/assets/icons/sprite.svg && svg-sprite-generate -d src/assets/icons -o src/assets/icons/sprite.svg",
    "postinstall": "yarn generate:icons"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.11",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/jest": "^23.1.5",
    "@types/lodash": "^4.14.112",
    "@types/node": "^10.5.2",
    "@types/react": "^16.4.6",
    "@types/react-datepicker": "^1.1.5",
    "@types/react-dom": "^16.0.6",
    "@types/react-redux": "^6.0.4",
    "@types/react-redux-toastr": "^7.1.0",
    "@types/react-router-dom": "^4.2.7",
    "@types/react-router-redux": "^5.0.15",
    "@types/reactstrap": "^6.0.0",
    "@types/redux-form": "^7.4.1",
    "@types/redux-immutable-state-invariant": "^2.0.4",
    "coveralls": "^3.0.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "react-test-renderer": "^16.4.1",
    "typescript": "^2.9.2"
  },
  "proxy": {
    "/api": {
      "target": "http://localhost:3001"
    }
  }
}
